<h1>Family Detail</h1>

<% form_for @family, :html => {:multipart => true} do |f| %>
  <%= f.error_messages %>

  <%= render :partial => f -%>
  <p>
    <%= f.submit 'Update', :disable_with => "Updating...", :class=>"button" %>
    <%= link_to 'Back', families_path, :class=>"button" %>
  </p>
<% end %>

